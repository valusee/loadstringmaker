-- Paste your link here
local linkMaker = [[
--// Paste your link here
]]

-- Function to create loadstring from a link
local function createLoadstringLink(link)
return 'loadstring(game
("' .. link .. '"))()'
end

-- Convert linkMaker to loadstring format
local loadstringLink = createLoadstringLink(linkMaker)

-- Print message using warn
warn("Loadstring Done.")

-- Print the loadstring
print(loadstringLink)

-- Copy to clipboard (using setclipboard)
if setclipboard then
setclipboard(loadstringLink)
end
